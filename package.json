{
  "name": "chor-js",
  "version": "0.4.0",
  "description": "A BPMN 2.0 choreography diagram rendering toolkit and web modeler.",
  "keywords": [
    "choreography model",
    "choreography",
    "modeler",
    "browser",
    "choreo-js",
    "toolkit",
    "bpmn"
  ],
  "main": "index.js",
  "source": "lib",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "karma start",
    "bundle-css": "parcel build assets/styles/chor-js.css -d dist/assets --public-url ./",
    "bundle-production": "parcel build index.js -d dist --global ChorJS --out-file chor-js.production.min.js --no-source-maps --public-url ./",
    "bundle-development": "parcel build index.js -d dist --global ChorJS --out-file chor-js.development.js --no-minify --public-url ./",
    "prepublishOnly": "npm run bundle-css && npm run bundle-development && npm run bundle-production"
  },
  "author": {
    "name": "Jan Ladleif",
    "url": "https://github.com/jan-ladleif"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bptlab/chor-js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babelify": "^10.0.0",
    "browserify": "^16.3.0",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-bpmn-io": "^0.8.2",
    "karma": "^4.2.0",
    "karma-browserify": "^6.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^6.2.0",
    "mocha-test-container-support": "^0.2.0",
    "parcel-bundler": "^1.12.3",
    "puppeteer": "^1.19.0",
    "stringify": "^5.2.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "bpmn-js": "^3.4.3",
    "core-js": "^3.1.4",
    "diagram-js": "^3.3.1",
    "inherits": "^2.0.4",
    "min-dash": "^3.5.0",
    "tiny-svg": "^2.2.0"
  }
}
